<% content_for :title do %>
    <%= t('.create') %>
<% end %>

<% content_for :page_title do %>    
    <a href="<%= erp_targets.backend_targets_path %>">
    <i class="icon-target"></i> <%= t('.targets') %>
    </a> / <%= t('.create') %>
<% end %>

<div class="row">
    <div class="col-md-12">
        <% if @target.errors.any? %>
            <%= erp_form_control("form_error_message") %>
        <% end %>
        
        <!-- BEGIN SAMPLE FORM PORTLET-->
        <%= render 'form', target: @target %>
        <!-- END SAMPLE FORM PORTLET-->
    </div>
</div>